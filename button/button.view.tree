$eve_button $eve_flex
	interactive true
	colors? \primary
	variant? \solid
	size? \m
	
	justify_content \center
	align_items \center
	sub /
		<= Label $mol_text
			text <= label \
	- взял с кнопки mol_button
	click? null
	event_click? null
	event *
		^
		click? <=> activate? <=> event_activate? null
		dblclick? <=> clicks? null
		keydown? <=> key_press? <=> event_key_press? null
	attr *
		^
		disabled <= disabled false
		role \button
		tabindex <= tab_index 0
		title <= hint_safe <= hint \
